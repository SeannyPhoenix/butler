{
  "name": "@seannyphoenix/butler",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "start": "turbo run start --parallel",
    "server": "npm run start -w server",
    "web": "npm run start -w web"
  },
  "devDependencies": {
    "eslint-config-google": "^0.14.0",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.1.2"
}